WindowTitle {Volume}
WindowSize  90 120
Colorset 9
Font "xft:Tinos Nerd Font:size=12" 

Init
 Begin
  Set $Title = (GetOutput {exec echo "ï€¨ "} 1 -1)
  Set $Volume = (GetOutput {exec amixer get Master | grep "Left" | grep "%" | tr " " "\n" | grep "%" | tr -d %[]} 1 -1)
  ChangeTitle 1 $Title
  ChangeTitle 2 $Volume
End

PeriodicTasks
 Begin 
  If (RemainderOfDiv (GetTime) 1)==0 Then
  Begin
    Set $Volume = (GetOutput {exec amixer get Master | grep "Left" | grep "%" | tr " " "\n" | grep "%" | tr -d %[]} 1 -1)
    ChangeTitle 2 $Volume
  End
End

Widget 1
Property
 Position 9 10
 Size 80 50
 Font "xft:Tinos Nerd Font:size=12"
 Type ItemDraw
 Flags NoReliefString
 Title {}
Main
 Case message of
  SingleClic :
  Begin
  End
End

